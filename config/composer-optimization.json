{
    "config": {
        "cache-dir": "/workspace/cache/composer",
        "cache-files-dir": "/workspace/cache/composer/files",
        "cache-repo-dir": "/workspace/cache/composer/repo",
        "cache-vcs-dir": "/workspace/cache/composer/vcs",
        "cache-files-ttl": 86400,
        "cache-files-maxsize": "1GB",
        "optimize-autoloader": true,
        "classmap-authoritative": true,
        "apcu-autoloader": true,
        "process-timeout": 300,
        "use-include-path": true,
        "discard-changes": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "*": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.org",
            "options": {
                "ssl": {
                    "verify_peer": false
                }
            }
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true
}